{
  "name": "@burand/core",
  "version": "1.0.0",
  "description": "Fornece métodos essenciais para a manipulação de uma coleção no Firebase.",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tsc --noEmit && eslint --ext .js,.ts .",
    "build": "npm run clean && tsc"
  },
  "homepage": "https://github.com/flavianoredressa/burand-core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flavianoredressa/burand-core.git"
  },
  "files": [
    "lib/"
  ],
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "exceptions": [
        "./lib/exceptions"
      ],
      "firestore": [
        "./lib/firestore"
      ],
      "http": [
        "./lib/http"
      ],
      "middlewares": [
        "./lib/middlewares"
      ],
      "typings": [
        "./lib/typings"
      ],
      "utils": [
        "./lib/utils"
      ]
    }
  },
  "exports": {
    "./exceptions": {
      "types": "./lib/exceptions/index.d.ts",
      "import": "./lib/exceptions/index.js"
    },
    "./firestore": {
      "types": "./lib/firestore/index.d.ts",
      "import": "./lib/firestore/index.js"
    },
    "./http": {
      "types": "./lib/http/index.d.ts",
      "import": "./lib/http/index.js"
    },
    "./middlewares": {
      "types": "./lib/middlewares/index.d.ts",
      "import": "./lib/middlewares/index.js"
    },
    "./typings": {
      "types": "./lib/typings/index.d.ts",
      "import": "./lib/typings/index.js"
    },
    "./utils": {
      "types": "./lib/utils/index.d.ts",
      "import": "./lib/utils/index.js"
    },
    "./tsconfig.json": {
      "default": "./tsconfig.json"
    }
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2"
  }
}
